{
  "name": "ffo-auto-script",
  "productName": "ffo-auto-script",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "npx --yes --package @electron-forge/cli@latest electron-forge start",
    "start:utf8": "powershell -ExecutionPolicy Bypass -File scripts/start_utf8.ps1",
    "setup:ia32": "powershell -ExecutionPolicy Bypass -File scripts/setup_ia32.ps1",
    "dm:verify32": "powershell -ExecutionPolicy Bypass -File scripts/verify_dm_reg_ia32.ps1",
    "package": "npx --yes --package @electron-forge/cli@latest electron-forge package",
    "make": "npx --yes --package @electron-forge/cli@latest electron-forge make",
    "publish": "npx --yes --package @electron-forge/cli@latest electron-forge publish",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.tsx,.ts,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dll:arch": "powershell -ExecutionPolicy Bypass -File scripts/check_dll_arch.ps1",
    "dm:register32": "powershell -ExecutionPolicy Bypass -File scripts/register_dm32.ps1",
    "env:check": "set ENV_CHECK_ONLY=1 && npx --yes --package @electron-forge/cli@latest electron-forge start",
    "env:check:ps": "powershell -ExecutionPolicy Bypass -File scripts/env_check.ps1",
    "env:check:js": "node scripts/env_check.js"
  },
  "keywords": [],
  "author": {
    "name": "月牙魔法师",
    "email": "617938514@qq.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-webpack": "^6.4.2",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.11.0",
    "electron": "^13.6.9",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "node-loader": "^2.1.0",
    "prettier": "^3.7.4",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "winax": "^3.6.2"
  }
}
