{
  "name": "ffo-auto-script",
  "productName": "ffo-auto-script",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "npx --yes --package @electron-forge/cli@latest electron-forge start",
    "start:utf8": "powershell -ExecutionPolicy Bypass -File scripts/start_utf8.ps1",
    "setup:ia32": "powershell -ExecutionPolicy Bypass -File scripts/setup_ia32.ps1",
    "dm:verify32": "powershell -ExecutionPolicy Bypass -File scripts/verify_dm_reg_ia32.ps1",
    "package": "npx --yes --package @electron-forge/cli@latest electron-forge package --arch=ia32",
    "make": "npx --yes --package @electron-forge/cli@latest electron-forge make --arch=ia32",
    "publish": "npx --yes --package @electron-forge/cli@latest electron-forge publish",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.tsx,.ts,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dll:arch": "powershell -ExecutionPolicy Bypass -File scripts/check_dll_arch.ps1",
    "dm:register32": "powershell -ExecutionPolicy Bypass -File scripts/register_dm32.ps1",
    "env:check": "set ENV_CHECK_ONLY=1 && electron-forge start",
    "env:check:ps": "powershell -ExecutionPolicy Bypass -File scripts/env_check.ps1",
    "env:check:js": "node scripts/env_check.js",
    "generate:icon": "node scripts\\generate_icon.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [],
  "author": {
    "name": "MoonMage",
    "email": "617938514@qq.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-wix": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-webpack": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.11.0",
    "electron": "^13.6.9",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jimp": "^1.6.0",
    "less": "^4.5.1",
    "less-loader": "^12.3.0",
    "node-loader": "^2.1.0",
    "pm2": "^6.0.14",
    "prettier": "^3.7.4",
    "style-loader": "^3.3.4",
    "to-ico": "^1.1.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "axios": "^1.13.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-winstaller": "^5.1.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.0",
    "openai": "^6.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "winax": "^3.6.2"
  }
}
